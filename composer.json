{
    "name": "blob/laravel-mor",
    "description": "A simple Laravel interface for interacting with the mor API.",
    "keywords": ["laravel", "mor", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bloby"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "mtownsend/xml-to-array": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MOR\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
